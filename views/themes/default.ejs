<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="<%= resumeData.name %>">
    <title>
        <%= resumeData.name %> - <%= resumeData.title %>
    </title>
    <link href="../../themes/default/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Saira+Extra+Condensed:100,200,300,400,500,600,700,800,900"
        rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i"
        rel="stylesheet">
    <link href="../../themes/default/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="../../themes/default/vendor/devicons/css/devicons.min.css" rel="stylesheet">
    <link href="../../themes/default/vendor/simple-line-icons/css/simple-line-icons.css" rel="stylesheet">
    <link href="../../themes/default/css/resume.min.css" rel="stylesheet">
</head>

<body id="page-top">
    <nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top" id="sideNav"> <a
            class="navbar-brand js-scroll-trigger" href="#page-top"> <span class="d-block d-lg-none">
                <%= resumeData.name %>
            </span><span class="d-none d-lg-block">
                <img style="
                  width: 160px;
                  height: 160px;
                  border-radius: 50%;
                  overflow: hidden;
                  display: block;
                  margin: 0 auto;
                  box-shadow: 0 0 0 5px black;
                  margin-bottom: 30px;
                  filter: brightness(110%) contrast(110%) blur(0.5px);
                  object-fit: cover;
                " src="<%= resumeData.profileImage %>" alt="">
              </span>
               </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"> <span
                class="navbar-toggler-icon"></span> </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav">
                <li class="nav-item"> <a class="nav-link js-scroll-trigger" href="#about">About</a> </li>
                <li class="nav-item"> <a class="nav-link js-scroll-trigger" href="#education">Education</a> </li>
                <li class="nav-item"> <a class="nav-link js-scroll-trigger" href="#experience">Experience</a> </li>
                <li class="nav-item"> <a class="nav-link js-scroll-trigger" href="#skills">Skills</a> </li>
                <li class="nav-item"> <a class="nav-link js-scroll-trigger" href="#project">project</a> </li>
            </ul>
        </div>
    </nav>
    
    
    <div class="container-fluid p-0">

        <!-- Profile Photo -->
        <div class="profile-image-container d-lg-none text-center mt-4">
          <div style="width: 160px; height: 160px; border-radius: 50%; overflow: hidden; display: flex; justify-content: center; align-items: center; margin: 0 auto; box-shadow: 0 0 0 5px black;">
            <img class="img-fluid img-profile mx-auto" src="<%= resumeData.profileImage %>" alt="" style="width: 100%; height: 100%; object-fit: cover;">
          </div>
        </div>
      </div>
        
        

        <section class="resume-section p-3 p-lg-5 d-flex d-column" id="about">
            <div class="my-auto">
                <h1 class="mb-0"><span class="text-primary">
                        <%= resumeData.name %>
                    </span> </h1>
                <h4 class="mb-0"><span class="text-primary">
                        <%= resumeData.title %>
                    </span> </h1>
                <div class="subheading mb-0">
                     <a href="mailto:<%= resumeData.email %>">
                            <%= resumeData.email %>
                        </a>
                </div>
                <div class="mb-3">
                    <a target=”_blank” href="<%= resumeData.resume %>"><button type="button"
                            class="btn btn-dark resume">Resume</button></a>
                </div>
                <p class="mb-5">
                    <%= resumeData.about %>
                </p>
                <ul class="list-inline list-social-icons mb-0">
                    <li class="list-inline-item"> <a href="<%= resumeData.profiles.site %>"> <span
                                class="fa-stack fa-lg"> <i class="fa fa-circle fa-stack-2x"></i> <i
                                    class="fa fa-twitter fa-stack-1x fa-inverse"></i> </span> </a> </li>
                    <li class="list-inline-item"> <a href="<%= resumeData.profiles.linkedin %>"> <span
                                class="fa-stack fa-lg"> <i class="fa fa-circle fa-stack-2x"></i> <i
                                    class="fa fa-linkedin fa-stack-1x fa-inverse"></i> </span> </a> </li>
                    <li class="list-inline-item"> <a href="<%= resumeData.profiles.github %>"> <span
                                class="fa-stack fa-lg">
                                <i class="fa fa-circle fa-stack-2x"></i> <i
                                    class="fa fa-github fa-stack-1x fa-inverse"></i> </span> </a> </li>
                </ul>
            </div>
        </section>
                                                    
        <!-- Education -->
        <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="education">
            <div class="my-auto">
                <h2 class="mb-5">Education</h2>
                <% resumeData.education.forEach(function(item) { %>
                    <div class="resume-item d-flex flex-column flex-md-row">
                        <div class="resume-content mr-auto">
                            <h3 class="mb-0">
                                <%= item.institution %>
                            </h3>
                            <div class="subheading mb-3">
                                <%= item.degree %>
                            </div>
                            <p>Grade <%= item.grade %>
                            </p>
                        </div>
                        <div class="resume-date text-md-right"> <span class="text-primary">
                                <%= item.year %>
                            </span> </div>
                    </div>
                    <% }); %>
            </div>
        </section>

        <!-- Experience -->
        <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="experience">
            <div class="my-auto">
                <h2 class="mb-5">Experience</h2>
                <% resumeData.experience.forEach(function(item) { %>
                    <div class="resume-item d-flex flex-column flex-md-row">
                        <div class="resume-content mr-auto">
                            <h3 class="mb-0">
                                <%= item.position %>
                            </h3>
                            <div class="subheading mb-3">
                                <%= item.employer %>
                            </div>
                            <p>
                                <%= item.description %>
                            </p>
                        </div>
                    </div>
                    <% }); %>
            </div>
        </section>

        <!-- Skills -->
        <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="skills">
            <div class="my-auto">
              <h2 class="mb-5">Skills</h2>
              <div style="display: flex; flex-wrap: wrap;">
                <% let skillsChunks = []; %>
                <% for (let i = 0; i < resumeData.skills.length; i += 5) { %>
                  <% skillsChunks.push(resumeData.skills.slice(i, i + 5)); %>
                <% } %>
                <% skillsChunks.forEach(skillChunk => { %>
                  <div class="skills-row">
                    <% skillChunk.forEach(skill => { %>
                    <div style=" height: 24px;display: flex;align-items: center;justify-content: center;padding: 5px;box-sizing: border-box;margin-right: 10px;margin-bottom: 10px;background-color: #f0f0f0;border-radius: 4px;min-width: calc(1ch + 2px);"
                    ><%= skill %>
                    </div>
                    <% }); %>
                  </div>
                <% }); %>
              </div>
            </div>
          </section>
        

        <!-- Projects -->
        <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="project">
            <div class="my-auto">
                <h2 class="mb-5">Project</h2>
                <% resumeData.projects.forEach(function(project) { %>
                    <div class="resume-item d-flex flex-column flex-md-row mb-5">
                        <div class="resume-content mr-auto">
                            <h3 class="mb-0"><a href="#">
                                    <%= project.name %>
                                </a></h3>
                            <div class="subheading mb-3">Tech: <%= project.tech.join(', ') %></div>
                            <p><%= project.description %></p>
                        </div>
                    </div>
                <% }); %>              
        </section>

    </div>

    <script src="../../themes/default/vendor/jquery/jquery.min.js"></script>
    <script src="../../themes/default/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="../../themes/default/vendor/jquery-easing/jquery.easing.min.js"></script>
    <script src="../../themes/default/js/resume.min.js"></script>

</body>


</html>